{
  "manifest_version": 3,
  "name": "RemoveAdsYoutube",
  "version": "1.0",
  "description": "",

  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["script.js"],
      "matches": ["https://www.youtube.com/*"]
    }
  ],
  "permissions": ["activeTab", "storage", "tabs", "scripting"]
}
